<%= simple_form_for [:admin, @job] do |f| %>

  <%= f.input :title, label: "标题"%>
  <%= f.input :description, label: "简介" %>
  <%= f.input :wage_lower_bound, label: "最低薪资" %>
  <%= f.input :wage_upper_bound, label: "最高薪资" %>
  <%= f.input :contact_email, label: "联系邮箱" %>
  <%= f.input :is_hidden %>
  <%= f.submit "Submit", class: "btn btn-primary"%>
<% end %>
